<% autocomplete_classes=get_field_autocomplete_class(f.object.model,key) %>
<% if f.object.class.multiple? key %>
  <%= f.input key, as: :multi_value, input_html: { class: ['form-control']+autocomplete_classes }, required: f.object.required?(key) %>
<% else %>
  <%= f.input key, input_html: { class: autocomplete_classes}, required: f.object.required?(key) %>
<% end %>
